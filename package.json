{
  "name": "essentials",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "config"
  ],
  "scripts": {
    "dev:web": "cd apps/web && npm run dev",
    "dev:api": "cd apps/api && npm run dev",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:api\"",
    "build:web": "cd apps/web && npm run build",
    "build:api": "cd apps/api && npm run build",
    "build:mobile": "cd apps/web && npm run build && cd ../mobile && npx cap sync",
    "build:ios": "cd apps/web && npm run build && cd ../mobile && npx cap sync && npx cap build ios",
    "build:android": "cd apps/web && npm run build && cd ../mobile && npx cap sync && npx cap build android",
    "open:ios": "cd apps/mobile && npx cap open ios",
    "open:android": "cd apps/mobile && npx cap open android",
    "run:ios": "cd apps/mobile && npx cap run ios",
    "run:android": "cd apps/mobile && npx cap run android",
    "install:all": "npm install && cd apps/web && npm install && cd ../mobile && npm install && cd ../api && npm install",
    "deploy:web": "cd apps/web && vercel --prod --yes",
    "deploy:api": "cd apps/api && vercel --prod --yes"
  },
  "dependencies": {
    "@capacitor/android": "^7.3.0",
